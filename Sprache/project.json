{
  "title": "Sprache",
  "version": "2.1.2-*",
  "authors": [ "Nicholas Blumhardt", "Contributors" ],
  "copyright": "Copyright Sprache Contributors 2017",
  "description": "Sprache is a simple, lightweight library for constructing parsers directly in C# code.",

  "buildOptions": {
    "optimize": true,
    "outputName": "Sprache",
    "xmlDoc": true
  },

  "packOptions": {
    "owners": [ "Nicholas Blumhardt", "Mike Hadlow" ],
    "requireLicenseAcceptance": false,
    "summary": "Sprache is a simple, lightweight library for constructing parsers directly in C# code.",
    "tags": [ "Parser", "Parsers", "Monad" ],
    "projectUrl": "https://github.com/sprache/Sprache",
    "licenseUrl": "https://github.com/sprache/Sprache/blob/master/licence.txt",
    "iconUrl": "https://avatars1.githubusercontent.com/u/1999078",
    "repository": {
      "type": "git",
      "url": "https://github.com/sprache/Sprache"
    }
  },

  "frameworks": {
    "net3.5": {
      "buildOptions": {
        "define": [ "STRING_IS_ENUMERABLE" ]
      }
    },
    "net4.0": {
      "buildOptions": {
        "define": [ "STRING_IS_ENUMERABLE", "STRING_JOIN_ENUMERABLE" ]
      }
    },
    "sl5": {
      "frameworkAssemblies": {
        "System": "5.0.5",
        "System.Core": "5.0.5",
        "mscorlib": "5.0.5"
      },
      "buildOptions": {
        "define": [ "STRING_IS_ENUMERABLE", "STRING_JOIN_ENUMERABLE" ]
      }
    },
    "netstandard1.0": {
      "dependencies": {
        "System.Globalization": "4.0.11",
        "System.Linq": "4.1.0",
        "System.Runtime": "4.1.0",
        "System.Text.RegularExpressions": "4.1.0"
      },
      "buildOptions": {
        "define": [ "STRING_JOIN_ENUMERABLE" ]
      }
    }
  }
}
